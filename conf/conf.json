{
    "db":{
        "host":"SMLPOPENMAPS2",
        "port":"5432",
        "name":"ome2_hvlsp_template",
        "user":"ome2_user",
        "pwd":"ome2_user"
    },
    "landmask": {
        "schema": "ib",
        "table": "national_admin_extent",
        "fields":{
            "country": "country",
            "geometry": "geom"
        }
    },
    "boundary": {
        "schema": "ib",
        "table": "international_boundary_line",
        "fields":{
            "country": "country",
            "geometry": "geom",
            "type": "boundary_type"
        },
        "boundary_type_values":{
            "international": "international_boundary"
        }
    },
    "data": {
        "common_fields": {
            "country": "country",
            "geometry": "geom",
            "id": "inspireid",
            "step": "w_step"
        },
        "history": {
            "suffix": "_wh",
            "fields": {
                "modification_type": "w_modification_type",
                "modification_step": "w_modification_step"
            }
        },
        "working": {
            "ids_suffix": "_w_ids",
            "suffix": "_w"
        },
        "themes": {
            "tn": {
                "schema": "tn",
                "h_schema": "tn",
                "w_schema": "public",
                "tables": [
                    "road_link",
                    "road",
                    "road_node",
                    "road_service_point",
                    "road_service_area",
                    "marker_post",
                    "railway_link",
                    "railway_line",
                    "railway_station_point",
                    "railway_station_area",
                    "aerodrome_point",
                    "aerodrome_area",
                    "runway_line",
                    "runway_point",
                    "ferry_crossing",
                    "port_point",
                    "port_area"
                ]
            },
            "au": {
                "schema": "au",
                "h_schema": "au",
                "w_schema": "public",
                "tables": [
                    "administrative_boundary",
                    "administrative_area",
                    "administrative_unit_1",
                    "administrative_unit_2",
                    "administrative_unit_3",
                    "administrative_unit_4",
                    "administrative_unit_5",
                    "administrative_unit_6",
                    "administrative_hierarchy"
                ]
            },
            "ib": {
                "schema": "ib",
                "h_schema": "ib",
                "w_schema": "public",
                "tables": [
                    "international_boundary_line",
                    "international_boundary_node",
                    "landmask"
                ]
            },
            "hy": {
                "schema": "hy",
                "h_schema": "hy",
                "w_schema": "public",
                "tables": [
                    "watercourse_link",
                    "watercourse",
                    "hydro_node",
                    "watercourse_area",
                    "standing_water",
                    "drainage_basin",
                    "shoreline",
                    "shore",
                    "dam_area",
                    "dam_line",
                    "dam_point",
                    "lock_area",
                    "lock_line",
                    "lock_point",
                    "falls_line",
                    "falls_point"
                ]
            }
        }
    },
    "border_extraction": {
        "where1": "ST_intersects(${data.common_fields.geometry},ST_SetSRID(ST_GeomFromText('POLYGON((3.38928 50.36368,3.38928 50.71280,3.88366 50.71280,3.88366 50.36368,3.38928 50.36368))'),4326))",
        "where2": "ST_intersects(${data.common_fields.geometry},ST_SetSRID(ST_GeomFromText('POLYGON((3808951 3040674,3808951 3101076,3882202 3101076,3882202 3040674,3808951 3040674))'),3035))",
        "where3": "ST_intersects(${data.common_fields.geometry},ST_SetSRID(ST_GeomFromText('POLYGON((3868601 3040674,3868601 3049417,3882202 3049417,3882202 3040674,3868601 3040674))'),3035))",
        "where": "ST_intersects(${data.common_fields.geometry},ST_SetSRID(ST_GeomFromText('POLYGON((3873474.9 3041465.1,3873474.9 3041743.9,3873766.5 3041743.9,3873766.5 3041465.1,3873474.9 3041465.1))'),3035))"
    }
