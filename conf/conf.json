{
    "db":{
        "host":"SMLPOPENMAPS2",
        "port":"5432",
        "name":"ome2_v0",
        "user":"ome2_user",
        "pwd":"ome2_user"
    },
    "landmask": {
        "schema": "ome2_ib",
        "table": "national_admin_extent",
        "fields":{
            "country": "country",
            "geometry": "geom"
        }
    },
    "boundary": {
        "schema": "ome2_ib",
        "table": "international_boundary_line",
        "fields":{
            "country": "country",
            "geometry": "geom"
        }
    },
    "data": {
        "common_fields": {
            "country": "country",
            "geometry": "geom",
            "id": "inspireid",
            "step": "w_step"
        },
        "history": {
            "suffix": "_wh",
            "fields": {
                "modification_type": "w_modification_type",
                "modification_step": "w_modification_step"
            }
        },
        "working": {
            "ids_suffix": "_w_ids",
            "suffix": "_w"
        },
        "themes": {
            "tn": {
                "schema": "tn",
                "h_schema": "tn",
                "w_schema": "public",
                "tables": [
                    "road_link"
                ]
            },
            "au": {
                "schema": "au",
                "h_schema": "au",
                "w_schema": "public",
                "tables": [
                    "administrative_boundary",
                    "administrative_area",
                    "administrative_unit_1",
                    "administrative_unit_2",
                    "administrative_unit_3",
                    "administrative_unit_4",
                    "administrative_unit_5",
                    "administrative_unit_6"
                ]
            }
        }
    },
    "border_extraction": {
        "where1": "ST_intersects(${data.common_fields.geometry},ST_SetSRID(ST_GeomFromText('POLYGON((3.38928 50.36368,3.38928 50.71280,3.88366 50.71280,3.88366 50.36368,3.38928 50.36368))'),4326))"
    }
}