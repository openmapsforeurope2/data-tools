{
    "db":{
        "host":"SMWPERMEGMFT1",
        "port":"5432",
        "name":"ome2_v0",
        "user":"postgres",
        "pwd":"postgres"
    },
    "landmask": {
        "schema": "ome2_ib",
        "table": "national_admin_extent",
        "fields":{
            "country": "country",
            "geometry": "geom"
        }
    },
    "boundary": {
        "schema": "ome2_ib",
        "table": "international_boundary_line",
        "fields":{
            "country": "country",
            "geometry": "geom"
        }
    },
    "data": {
        "common_fields": {
            "country": "country",
            "geometry": "geom",
            "id": "inspireid",
            "step": "w_step"
        },
        "history": {
            "suffix": "_wh",
            "fields": {
                "modification_type": "w_modification_type",
                "modification_step": "w_modification_step"
            }
        },
        "working": {
            "ids_suffix": "_w_ids",
            "suffix": "_w"
        },
        "themes": {
            "tn": {
                "schema": "ome2_tn",
                "h_schema": "ome2_tn",
                "w_schema": "public",
                "tables": [
                    "road_link"
                ]
            }
        }
    },
    "border_extraction": {
        "where1": "ST_intersects(${data.common_fields.geometry},ST_SetSRID(ST_GeomFromText('POLYGON((3.38928 50.36368,3.38928 50.71280,3.88366 50.71280,3.88366 50.36368,3.38928 50.36368))'),4326))"
    }
}